{
  "name": "task-prompt",
  "version": "1.0.0",
  "description": "Task Prompt Application",
  "scripts": {
    "start": "concurrently \"pnpm server\" \"pnpm client\"",
    "server": "cd server && pnpm dev",
    "client": "cd client && pnpm dev",
    "install-all": "pnpm install && cd server && pnpm install && cd ../client && pnpm install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
